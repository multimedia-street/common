{
    "name": "multimedia-street/common",
    "type": "library",
    "description": "Common usage for Multimedia Street Projects",
    "keywords": [
        "multimedia-street",
        "common"
    ],
    "homepage": "https://github.com/multimedia-street/common",
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Are Galinada",
            "email": "jayaregalinada@gmail.com",
            "homepage": "http://jayaregalinada.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9|~7.0",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "symfony/http-kernel": "2.*",
        "predis/predis": "1.*",
        "orangehill/iseed": "2.*",
        "intervention/image": "2.*",
        "intervention/imagecache": "2.*",
        "itsgoingd/clockwork": "1.*",
        "doctrine/dbal": "2.*",
        "filp/whoops": "2.*",
        "barryvdh/laravel-dompdf": "0.6.*",
        "barryvdh/laravel-cors": "0.8.*",
        "maatwebsite/excel": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Mmstreet\\Common\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmstreet\\Common\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
